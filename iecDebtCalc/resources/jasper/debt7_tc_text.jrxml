					<textElement>
						<font fontName="MYuen HK Medium" size="12" isBold="false" isItalic="false" isPdfEmbedded="true"/>
					</textElement>
					<text><![CDATA[你可考慮以下建議管理債務:]]></text>
				</staticText>
				<staticText>
					<reportElement uuid="cd8770b1-6f00-4d5d-8658-3eef8afa7aa0" x="10" y="56" width="761" height="20"/>
