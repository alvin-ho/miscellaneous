				<imageExpression><![CDATA["iec-plusMoney.png"]]></imageExpression>
			</image>
			<image>
				<reportElement uuid="9104dffd-2a83-4ec0-9797-7796e6fa86f9" x="69" y="236" width="5" height="5">
					<property name="local_mesure_unitwidth" value="pixel"/>
