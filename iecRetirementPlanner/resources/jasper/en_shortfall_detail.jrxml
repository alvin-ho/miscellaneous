					<font fontName="Gotham Rounded Book" size="10" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false"/>
				</textElement>
				<text><![CDATA[Generate income]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="81cfe358-ee0b-4a41-afb8-3b3b22387c46" x="78" y="356" width="444" height="50" forecolor="#414141">
